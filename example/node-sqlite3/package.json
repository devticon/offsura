{
  "name": "node-sqlite3",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "sqlite3": "^5.0.0",
    "ts-import": "^2.0.15-alpha.0",
    "typeorm": "^0.2.29",
    "websocket": "^1.0.32"
  },
  "devDependencies": {
    "@types/node": "^14.14.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "typescript-transform-paths": "^2.0.2"
  },
  "scripts": {
    "start": "ts-node src/index.ts",
    "offsura": "node ../../dist/bin/index.js"
  },
  "offsura": {
    "versionFilePath": ".offsura",
    "hasura": {
      "url": "https://hasura.test.novitus.devticon.com"
    },
    "replication": {
      "entitiesDir": "entities",
      "tables": [
        "product_categories",
        "products",
        "products_product_categories"
      ]
    },
    "typeorm": {
      "type": "sqlite",
      "database": "sqlite",
      "synchronize": true,
      "entities": [
        "entities/**.entity{.ts,.js}"
      ],
      "logging": true
    }
  }
}
